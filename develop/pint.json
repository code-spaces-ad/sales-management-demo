{
    "preset": "laravel",
    "exclude": [
        "database",
        "node_modules",
        "tests",
        "vendor"
    ],
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "increment_style": {
            "style": "pre"
        },
        "new_with_braces": true,
        "not_operator_with_successor_space": false,
        "no_superfluous_phpdoc_tags": false,
        "phpdoc_align": {
            "align": "left",
            "spacing": 1
        }
    }
}
